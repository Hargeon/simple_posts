<h1>Users info</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Age</th>
    <th>Gender</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= user.age %>
      </td>
      <td>
        <%= user.gender %>
      </td>
    </tr>
  <% end %>
</table>
